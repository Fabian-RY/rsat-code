----------------------------------------------------------------
--
select
	term1_id,
	term2_id,
	relationship_type_id,
	t1.id,
	t1.name,
	t2.id,
	t2.name 
from 
	term2term,
	term t1,
	term t2 
where
	t2.name like 'methionine metabolism%' 
--and
--	relationship_type_id=5
and 
	term1_id=t1.id 
and 
	term2_id=t2.id
limit 100
;