# 4196 motifs extracted

cat kkall.motif.transfac | grep -v -P "^BF  T\d+" | grep -v -P "^BS  R\d+" | grep -v "RL  TRANSFAC" | grep -v "RL   Matrices from" | perl -lne 's/M\d+\(TRANSFAC 2012.1\)\|//g; print'  > footprintDB.motif.transfac
rm kkall.motif.transfac

