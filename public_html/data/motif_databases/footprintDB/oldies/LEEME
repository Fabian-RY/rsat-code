# 232 motifs extracted

cat kkplants.motif.transfac | grep -v -P "^BF  T\d+" | grep -v -P "^BS  R\d+" | grep -v "RL  TRANSFAC" | perl -lne 's/M\d+\(TRANSFAC 2012.1\)\|//g; print' > footprintDB.plants.motif.transfac
rm kkplants.motif.transfac

